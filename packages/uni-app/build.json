[
  {
    "input": {
      "src/compiler/index.ts": "dist/uni.compiler.js"
    },
    "output": {
      "format": "cjs"
    },
    "external": [
      "@dcloudio/uni-cloud",
      "@dcloudio/uni-push",
      "@dcloudio/uni-stat"
    ]
  },
  {
    "input": {
      "src/index.ts": ["dist/uni-app.es.js", "dist/uni-app.cjs.js"]
    },
    "compilerOptions": {
      "declaration": true
    },
    "replacements": {
      "__X__": "false"
    }
  },
  {
    "input": {
      "src/index.ts": ["dist-x/uni-app.es.js", "dist-x/uni-app.cjs.js"]
    },
    "replacements": {
      "__X__": "true"
    }
  },
  {
    "input": {
      "src/worker/index.ts": ["dist-x/uni-worker.js"]
    },
    "output": {
      "format": "iife"
    },
    "replacements": {
      "__X__": "true"
    }
  }
]
